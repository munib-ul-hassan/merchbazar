        <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/splidejs/4.1.0/js/splide.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
        <script>
            $(document).ready(function () {
                var current = window.location.href;
                $('.header .main-menu ul li a').each(function () {
                    console.log($(this));
                    if (this.href === current) {
                        $(this).addClass('active');
                    }
                });
                $('#main-menu-btn').on('click', function (e) {
                    e.preventDefault();
                    $('.main-menu').slideToggle();
                });

                const getAllCart = async () => {
                    await axios.get('/api/cart').then((res)=>{
                        console.log(res.data);

                        $('.counterBadge').text(res.data.length)
                    }).catch((error)=>{
                        console.log(error)
                    })
                }

                getAllCart()
            });
            var splide = new Splide('.splide', {
                rewind: true,
                perPage: 4,
                pagination: false,
                arrows: false,
                gap: '1.7rem',
                autoplay: true,
                breakpoints: {
                    991: {
                        perPage: 3,
                    },
                    756: {
                        perPage: 2,
                    },
                    500: {
                        perPage: 1,
                    },
                }
            });

            splide.mount();

            
            
           
        </script>
